public class CubesOfFirstTen {
    public static void main(String[] args) {
        System.out.println("Кубы первых десяти положительных чисел:");

        for (int i = 1; i <= 10; i++) {
            int cube = i * i * i; // Вычисляем куб числа
            System.out.println("Куб числа " + i + " равен " + cube);
        }
    }
}

